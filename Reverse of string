#include <stdio.h>

#include <string.h>

int main() {

    char str[100];

    int length, i, j;

    char temp;

    

    scanf("%s", str);

    length = strlen(str);

    

    for (i = 0, j = length - 1; i < j; i++, j--) {

        temp = str[i];

        str[i] = str[j];

        str[j] = temp;

    }

  
